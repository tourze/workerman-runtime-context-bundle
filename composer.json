{
  "name": "tourze/workerman-runtime-context-bundle",
  "description": "Workerman Runtime Context bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/symfony-runtime-context-bundle": "0.0.*",
    "workerman/workerman": "^5.1",
    "workerman/coroutine": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\WorkermanRuntimeContextBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\WorkermanRuntimeContextBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "maglnet/composer-require-checker": "^4"
  }
}